SHELL	=/bin/sh
INCMOD	=/nwprod/lib/incmod/sfcio_4
SRCM	=sfchdr.f
INCM	=
SRCS	=
OBJS	=
MODS	=
FC	=ncepxlf90
FFLAGS	=-qnosave -O2   -qmaxmem=-1
LDFLAGS	=   
LIBS    = -L/nwprod/lib -lsfcio_4  -L/nwprod/lib -lbacio_4 -lw3_4
CMD	=global_sfchdr
$(CMD):	$(SRCM) $(OBJS)
	$(FC) -I $(INCMOD) $(FFLAGS) $(LDFLAGS) $(SRCM) $(OBJS) $(LIBS) -o $(CMD)
clean:
	-rm -f $(OBJS) $(MODS)
clobber:        clean
	-rm -f $(CMD)
void:   clobber
	-rm -f $(SRCS) $(SRCM) $(INCM) makefile
